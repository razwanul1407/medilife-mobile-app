{
  "name": "Medilife",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "start": "react-native start",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .ts,.tsx ./src -f stylish",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx ./src --fix",
    "test": "jest",
    "types": "tsc --noemit",
    "prettify": "prettier --write . \"**/*.{js,jsx,ts,tsx,json}\"",
    "sort": "npx sort-imports --write \"./src/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install",
    "build": "cd android && ./gradlew assembleRelease && cd ..",
    "release": "yarn build && cd android && ./gradlew bundleRelease && cd ..",
    "gradle:clean": "cd android/ && ./gradlew clean"
  },
  "lint-staged": {
    "*.{jsx,ts,tsx}": [
      "yarn prettify",
      "yarn lint:fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.4.7",
    "@react-native-clipboard/clipboard": "^1.11.2",
    "@react-native-community/netinfo": "^9.3.10",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "@reduxjs/toolkit": "^1.9.5",
    "@shopify/flash-list": "^1.4.3",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/styled-components": "^5.1.26",
    "@types/styled-components-react-native": "^5.2.1",
    "axios": "^1.4.0",
    "formik": "^2.4.2",
    "luxon": "^3.3.0",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-date-picker": "^4.2.13",
    "react-native-gesture-handler": "^2.11.0",
    "react-native-linear-gradient": "^2.7.3",
    "react-native-mmkv": "^2.8.0",
    "react-native-otp-inputs": "^7.4.0",
    "react-native-paper": "^5.8.0",
    "react-native-reanimated": "^3.3.0",
    "react-native-safe-area-context": "^4.6.0",
    "react-native-screens": "^3.20.0",
    "react-native-svg": "^13.9.0",
    "react-native-vector-icons": "^9.2.0",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/cli-platform-android": "^11.3.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/luxon": "^3.3.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.59.9",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.8.8",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-testing-library": "^6.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
